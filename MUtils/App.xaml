<Application x:Class="MUtils.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:MUtils="clr-namespace:MUtils">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/MDotNet.WPF;component/Themes/generic.xaml"/>
                <ResourceDictionary Source="/AvalonDock.Themes;component/themes/Dev2010.xaml"/>
                <ResourceDictionary>
                    <MUtils:AppBootstrapper x:Key="bootstrapper"/>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>            
            <Style TargetType="{x:Type Window}"/>            
        </ResourceDictionary>        
    </Application.Resources>
</Application>
